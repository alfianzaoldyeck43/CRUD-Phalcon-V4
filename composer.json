{
  "name": "gamalan/phalcon-boilerplate",
  "description": "Phalcon Boilerplate",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Gamal Abdul Nasser",
      "email": "gamalanpro@gmail.com"
    }
  ],
  "repositories": {
    "packagist.org": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
    "require": {
    		"guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        	"symfony/var-dumper": "2.7.*",
        	"vlucas/phpdotenv": "~2.0"
    },
    "autoload": {
        "psr-4": {
          "Application\\": "app/library",
          "Application\\Models\\": "app/models",
          "Application\\Controllers\\": "app/controllers",
          "Application\\Task\\": "app/task",
          "Application\\Provider\\": "app/provider",
          "Application\\Listener\\": "app/listener",
          "Application\\Router\\": "app/router"
        }
      },
      "autoload-dev": {
        "psr-4": {
          "Application\\Tests\\": "tests"
        }
      },
      "minimum-stability": "dev",
      "config": {
        "bin-dir": "bin",
        "process-timeout": 6000,
        "discard-changes": true
      }
}
